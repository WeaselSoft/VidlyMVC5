@model Vidly.ViewModel.MovieViewModel
@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Movies</h2>
<table class=“table table-bordered table-hover”>
    <thead>Movies</thead>
    @foreach (var movie in Model.Movies)
    {
        <tr>
            <td>@movie.Name</td>
        </tr>
    }
</table>
